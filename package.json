{
  "name": "embeddings",
  "version": "1.0.0",
  "description": "Embeddings API server with TypeScript, nodemon, and Jest",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:test": "nodemon src/embedding/test-embedding.ts",
    "test": "node --env-file=.env.test --experimental-modules node_modules/.bin/jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "express",
    "typescript",
    "nodejs",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@chroma-core/openai": "^0.1.7",
    "chromadb": "^3.0.17",
    "express": "^4.18.2",
    "openai": "^5.23.1",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/supertest": "^2.0.12",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
